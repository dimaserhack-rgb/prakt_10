num1 = int(input("Введите первое число:"))
while True:
    num2 = int(input("Введите второе число:"))
    if num1 >= num2:
        print("Второе число должно быть больше и не равно первому:")
    else:
        break

while True:
    num3 = int(input("Введите третье число:"))
    if num2 >= num3:
        print("Третье число должно быть больше и не равно второму:")
    else:
        print("Последовательность принята!")
        break
